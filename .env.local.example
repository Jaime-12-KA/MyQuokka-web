# 인증 관련 환경 변수
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-nextauth-secret-key

# MongoDB 연결 설정
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/myquokka?retryWrites=true&w=majority

# Google OAuth 인증 정보
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# OpenAI API 키
OPENAI_API_KEY=your-openai-api-key

# Stripe 결제 설정 (모의 구현이므로 더미 값 사용)
STRIPE_PUBLIC_KEY=pk_test_dummy_public_key
STRIPE_SECRET_KEY=sk_test_dummy_secret_key

# 애플리케이션 설정
NODE_ENV=development 